<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.spyk">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/spyk_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/spyk_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TravelInfoActivity"
            android:exported="true" />
        <activity
            android:name=".TravelActivity"
            android:exported="true" />
        <activity
            android:name=".LocatorActivity"
            android:exported="true" />
        <activity android:name=".LinkCollectorActivity" />
        <activity
            android:name=".ClickClickActivity"
            android:label="@string/title_activity_click_click" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>